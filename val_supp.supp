{
	ignore_libreadline_leaks
	Memcheck:Leak
	...
	obj:*/libreadline.so.*
}
{
    ignore_glibc_leaks
    Memcheck:Leak
    ...
    obj:*/libc.so.*
}
{
    suppress-libc-localization-all
    Memcheck:Leak
    match-leak-kinds: reachable
    fun:*alloc
    ...
    obj:*/libc-*.so*
    ...
    fun:main
}

{
    suppress-libc-gettext-all
    Memcheck:Leak
    match-leak-kinds: reachable
    fun:*alloc
    ...
    fun:bindtextdomain
    ...
    fun:main
}

{
    suppress-libc-textdomain
    Memcheck:Leak
    match-leak-kinds: reachable
    fun:*alloc
    ...
    fun:textdomain
    ...
    fun:main
}

{
    suppress-cat-xalignalloc-buffers
    Memcheck:Leak
    match-leak-kinds: reachable
    fun:*alloc
    ...
    fun:xalignalloc
    fun:main
}

{
    suppress-grep-internal-leaks
    Memcheck:Leak
    match-leak-kinds: reachable,possible
    fun:*alloc
    ...
    fun:dfaanalyze*
    ...
    fun:GEAcompile
    fun:main
}

{
    suppress-grep-kws-leaks
    Memcheck:Leak
    match-leak-kinds: reachable
    fun:*alloc
    ...
    fun:kwsalloc
    ...
    fun:GEAcompile
    fun:main
}

{
    suppress-libc-charset-conversion
    Memcheck:Leak
    match-leak-kinds: reachable
    fun:*alloc
    ...
    fun:__gconv_lookup_cache
    ...
    fun:main
}

{
    suppress-libc-io-std-buffers
    Memcheck:Leak
    match-leak-kinds: reachable
    fun:malloc
    fun:_IO_file_doallocate
    ...
    fun:main
}

{
   <setlocale>
   Memcheck:Leak
   match-leak-kinds: reachable
   fun:malloc
   fun:strdup
   fun:_nl_load_locale_from_archive
   fun:_nl_find_locale
   fun:setlocale
   fun:main
}
{
   <setlocale_malloc_strdup>
   Memcheck:Leak
   match-leak-kinds: reachable
   fun:malloc
   fun:strdup
   fun:setlocale
   fun:main
}
{
   <setlocale_malloc_archive>
   Memcheck:Leak
   match-leak-kinds: reachable
   fun:malloc
   fun:_nl_load_locale_from_archive
   fun:_nl_find_locale
   fun:setlocale
   fun:main
}
{
   <setlocale_malloc_composite>
   Memcheck:Leak
   match-leak-kinds: reachable
   fun:malloc
   fun:new_composite_name
   fun:setlocale
   fun:main
}
{
   setlocale_malloc
   Memcheck:Leak
   match-leak-kinds: reachable
   fun:malloc
   fun:_nl_intern_locale_data
   fun:_nl_load_locale_from_archive
   fun:_nl_find_locale
   fun:setlocale
   fun:main
}
